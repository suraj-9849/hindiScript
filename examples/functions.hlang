firseKaro add(a, b): number {
	wapas bhejo a + b
}

firseKaro multiply(x, y): number {
	wapas bhejo x * y
}

firseKaro greet(name): string {
	bol("Namaste")
	bol(name)
	wapas bhejo "Hello Baburao"
}

